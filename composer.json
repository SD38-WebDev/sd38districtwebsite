{
    "name": "drupal-composer/drupal-project",
    "description": "Project template for Drupal 8 projects with composer",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type": "package",
            "package": {
              "name": "recurser/jquery-simple-color",
              "version": "1.2.3",
              "type": "drupal-library",
              "source": {
                "url": "https://github.com/recurser/jquery-simple-color",
                "type": "git",
                "reference": "master"
              }
            }
          },
          {
              "type": "package",
              "package": {
                "name": "bgrins/spectrum",
                "version": "1.8.1",
                "type": "drupal-library",
                "source": {
                  "url": "https://github.com/bgrins/spectrum",
                  "type": "git",
                  "reference": "master"
                }
              }
            }
    ],
    "require": {
        "php": ">=7.0.8",
        "bgrins/spectrum": "^1.8",
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/addanother": "^1.0",
        "drupal/address": "^1.8",
        "drupal/admin_toolbar": "^2.0",
        "drupal/admin_toolbar_search": "^1.0",
        "drupal/auto_entitylabel": "^3.0",
        "drupal/backup_migrate": "^4.1",
        "drupal/better_search": "^1.5",
        "drupal/bg_image_formatter": "^1.14",
        "drupal/block_class": "^1.1",
        "drupal/block_class_select": "^1.1",
        "drupal/bootstrap_barrio": "^4.33",
        "drupal/bootstrap_datetime_picker": "^1.2",
        "drupal/bootstrap_layouts": "^5.1",
        "drupal/bootstrap_library": "^1.11",
        "drupal/calendar": "^1.0",
        "drupal/captcha": "^1.0",
        "drupal/ckwordcount": "^1.0",
        "drupal/color_field": "^2.4",
        "drupal/colorbox": "^1.6",
        "drupal/colorbutton": "^1.1",
        "drupal/config_ignore": "^2.3",
        "drupal/config_installer": "^1.8",
        "drupal/console": "^1.0.2",
        "drupal/console-extend-plugin": "0.9.5",
        "drupal/content_lock": "^2.2",
        "drupal/ctools": "^3.4",
        "drupal/custom_body_class": "^1.2",
        "drupal/date_popup": "^1.0",
        "drupal/delete_files": "^1.5",
        "drupal/devel_entity_updates": "^2.0",
        "drupal/display_field_copy": "^1.2",
        "drupal/draggableviews": "^1.2",
        "drupal/dropzonejs": "^2.1",
        "drupal/ds": "^3.6",
        "drupal/editor_advanced_link": "^1.7",
        "drupal/editor_file": "^1.4",
        "drupal/embed": "^1.3",
        "drupal/entity_browser": "^2.5",
        "drupal/entity_embed": "^1.1",
        "drupal/field_group": "^3.0",
        "drupal/file_delete": "^1.0",
        "drupal/filefield_sources": "1.x-dev@dev",
        "drupal/fontyourface": "^3.2",
        "drupal/fullcalendar_view": "^5.0",
        "drupal/google_analytics": "^2.4",
        "drupal/gtranslate": "^1.13",
        "drupal/honeypot": "^2.0",
        "drupal/inline_entity_form": "^1.0",
        "drupal/json_feed": "^1.2",
        "drupal/link_field_autocomplete_filter": "^1.11",
        "drupal/linkit": "^5.0",
        "drupal/media_bulk_upload": "^1.0",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/menu_block": "1.x-dev",
        "drupal/menu_block_title": "^1.1",
        "drupal/menu_breadcrumb": "^1.12",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/node_class": "^1.0",
        "drupal/notification": "^1.0",
        "drupal/oembed_providers": "^1.1",
        "drupal/paragraph_view_mode": "^2.0",
        "drupal/paragraphs": "^1.12",
        "drupal/path_redirect_import": "^1.0",
        "drupal/pathauto": "^1.8",
        "drupal/recaptcha": "^2.5",
        "drupal/redirect": "^1.6",
        "drupal/search_exclude": "^1.2",
        "drupal/semanticviews": "^2.2",
        "drupal/slick": "^2.1",
        "drupal/slick_views": "^2.1",
        "drupal/smart_date": "^3.2",
        "drupal/social_media_links": "^2.6",
        "drupal/summary_word_limit": "^1.0",
        "drupal/template_whisperer": "^2.3",
        "drupal/token": "^1.6",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_bootstrap": "^4.2",
        "drupal/views_bulk_edit": "^2.4",
        "drupal/views_bulk_operations": "^3.6",
        "drupal/views_slideshow": "^4.6",
        "drupal/webform": "^5.10",
        "drush/drush": "^10.5",
        "enyo/dropzone": "^5.5",
        "platformsh/config-reader": "^2.3",
        "recurser/jquery-simple-color": "^1.2",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "drupal/core-dev": "^8.8"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
          "locations": {
            "web-root": "web/"
          }
        },
        "patches": {
            "drupal/custom_body_class:^1.2": {
                "Patch to fix fatal errors on node revisions": "https://www.drupal.org/files/issues/2019-01-05/get-error-when-revert-node-2997700-3.patch"
            },
            "drupal/filefield_sources:1.x-dev@dev": {
                "Deprecated function: Cannot use parent when current class scope has no parent": "https://www.drupal.org/files/issues/2020-06-12/3151023-fix-entity-type-manager-dependency-injection.patch"
            }
        }
    }
}
