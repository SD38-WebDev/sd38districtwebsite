    "require-dev": {
        "drupal/core-dev": "^9",
        "drupal/devel": "^5.1",
        "drupal/stage_file_proxy": "^2.0",
        "kint-php/kint": "^5.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/console-extend-plugin": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": ["load.environment.php"]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": ["type:drupal-core"],
            "web/libraries/{$name}": ["type:drupal-library"],
            "web/modules/contrib/{$name}": ["type:drupal-module"],
            "web/profiles/contrib/{$name}": ["type:drupal-profile"],
            "web/themes/contrib/{$name}": ["type:drupal-theme"],
            "drush/Commands/contrib/{$name}": ["type:drupal-drush"]
        },
        "drupal-scaffold": {
          "locations": {
            "web-root": "web/"
          }
        },
        "patches": {
            "drupal/core": {
                "3391461: Harden the use of unserialize in InlineBlock via allowed classes": "https://git.drupalcode.org/project/drupal/-/merge_requests/4926.patch",
                "3293908: Expose Layout Builder Sections to entity view displays": "https://www.drupal.org/files/issues/2022-09-22/3293908-5.patch",
                "2942975: [PP-1] Expose Layout Builder data to REST and JSON:API": "https://www.drupal.org/files/issues/2023-02-07/2942975-228.patch"
            },
            "drupal/custom_body_class:^1.2": {
                "Patch to fix fatal errors on node revisions": "https://www.drupal.org/files/issues/2019-01-05/get-error-when-revert-node-2997700-3.patch"
            },
            "drupal/filefield_sources:1.x-dev@dev": {
                "Deprecated function: Cannot use parent when current class scope has no parent": "https://www.drupal.org/files/issues/2020-06-12/3151023-fix-entity-type-manager-dependency-injection.patch"
            },
            "drupal/entity_reference_revisions": {
                "Views doesn't recognize relationship to host": "https://www.drupal.org/files/issues/2021-11-12/2799479-163-relationship-host-id.patch"
            },
            "drupal/openid_connect": {
                "3332759: Use getRedirectUrl() method when building authorize query string": "https://www.drupal.org/files/issues/2023-01-11/openid-connect-use-getredirecturl.patch",
                "3355330: Trying to access array offset on value of type null": "https://git.drupalcode.org/project/openid_connect/-/merge_requests/82.diff"
            },
            "drupal/openid_connect_windows_aad": {
                "3400940: Warning: Undefined array key \"end_session_endpoint\" since 3322786": "https://www.drupal.org/files/issues/2023-11-11/3400940-03.patch",
                "3124961: Enforce HTTPS": "https://git.drupalcode.org/project/openid_connect_windows_aad/-/merge_requests/26.patch"
            }
        }
    }
}
